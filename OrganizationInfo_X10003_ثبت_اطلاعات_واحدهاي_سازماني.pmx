        <dyn_content><![CDATA[{"name":"ايجاد اطلاعات سازماني","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"54071044962d7a5d0ce3028024447747","name":"ايجاد اطلاعات سازماني","description":"","mode":"edit","script":{"type":"js","code":"//نمايش صفحه\n\n$(\"#54071044962d7a5d0ce3028024447747\").css({\"backgroundColor\":\"#ccccff\"});\n$(\"#54071044962d7a5d0ce3028024447747\").css({\"border\":\"3px double #336699\"});\n\n\n$(\"#title0000000001\").css({\"font-family\":\"b Titr\",\"color\":\"#ffffff\",\"background-color\":\"#336699\",\"border-radius\":\"5px\"});\n$(\"#title0000000001\").css({\"text-align\":\"center\"});\n\n$(\"#submit0000000001\").css({\"font-family\":\"b titr\"});\n\n$(\"#L0_Counter\").hide();\n$(\"#L1_Counter\").hide();\n$(\"#L2_Counter\").hide();\n$(\"#L3_Counter\").hide();\n$(\"#L4_Counter\").hide();\n$(\"#L5_Counter\").hide();\n$(\"#EX_Counter\").hide();\n\n$(\"#ORG_Master_Code\").hide();\n$(\"#ORG_L1_Code\").hide();\n$(\"#ORG_L2_Code\").hide();\n$(\"#ORG_L3_Code\").hide();\n$(\"#ORG_L4_Code\").hide();\n$(\"#ORG_L5_Code\").hide();\n\n$(\"#Org_Type_Show\").hide();\n$(\"#ORG_L1_Code_Show\").hide();\n$(\"#ORG_L2_Code_Show\").hide();\n$(\"#ORG_L3_Code_Show\").hide();\n$(\"#ORG_L4_Code_Show\").hide();\n$(\"#ORG_L5_Code_Show\").hide();\n\n$(\"#ORG_Type\").hide();\n$(\"#ORG_Type_Select\").hide();\n$(\"#ORG_L1_Code_Select\").hide();\n$(\"#ORG_L2_Code_Select\").hide();\n$(\"#ORG_L3_Code_Select\").hide();\n$(\"#ORG_L4_Code_Select\").hide();\n$(\"#ORG_L5_Code_Select\").hide();\n\n$(\"#Ext_Org_Type_Show\").hide();\n$(\"#ORG_Code\").hide();\n$(\"#ORG_Name\").hide();\n$(\"#button_code\").hide();\n$(\"#submit0000000001\").hide();\n$(\"#Org_Level\").hide();\n\n//پر كردن gridview  --- \n\nif ($(\"#Description\").getNumberRows() == 1 && $(\"#Description\").getValue (1,1) ==\"\") {\n\t$('#Description').getControl($(\"#Description\").getNumberRows(),'2').attr('disabled',true);\n  \t$(\"#Description\").setValue($(\"#txt_name\").getValue(),$(\"#Description\").getNumberRows(),'1');\n  }else{\n\t$(\"#Description\").addRow();\n\t$('#Description').getControl($(\"#Description\").getNumberRows(),'2').attr('disabled',true); \n    $(\"#Description\").setValue($(\"#txt_name\").getValue(),$(\"#Description\").getNumberRows(),'1');\n  };\n\n// كنترل داخلي يا خارجي بودن\n\n$(\"#Ext_Int_ID\").setOnchange(function(EI) {\n  \n  EI = $(\"#Ext_Int_ID\").getValue();\n  \n  switch (true) {\n      \n    case (EI == \"N\"):\n      $(\"#Org_Level\").show();\n      $(\"#Ext_Org_Type_Show\").hide();\n      $(\"#ORG_Code\").hide();\n      $(\"#ORG_Code\").setValue(\"\");\n      $(\"#ORG_Name\").hide();\n      $(\"#ORG_Name\").setValue(\"\");\n      $(\"#button_code\").hide();\n      $(\"#submit0000000001\").hide();\n      $(\"#Org_Level\").show();\n      break;\n      \n    case (EI == \"X\"):\n      \n      $(\"#Org_Level\").hide();\n      $(\"#ORG_Master_Code\").hide();\n\t  $(\"#ORG_L1_Code\").hide();\n\t  $(\"#ORG_L2_Code\").hide();\n\t  $(\"#ORG_L3_Code\").hide();\n\t  $(\"#ORG_L4_Code\").hide();\n\t  $(\"#ORG_L5_Code\").hide();\n      \n      $(\"#Org_Type_Show\").hide();\n      $(\"#ORG_L1_Code_Show\").hide();\n      $(\"#ORG_L2_Code_Show\").hide();\n      $(\"#ORG_L3_Code_Show\").hide();\n      $(\"#ORG_L4_Code_Show\").hide();\n      $(\"#ORG_L5_Code_Show\").hide();\n      \n      $(\"#ORG_Type\").hide();\n      $(\"#ORG_Type_Select\").hide();\n      $(\"#ORG_L1_Code_Select\").hide();\n      $(\"#ORG_L2_Code_Select\").hide();\n      $(\"#ORG_L3_Code_Select\").hide();\n      $(\"#ORG_L4_Code_Select\").hide();\n      $(\"#ORG_L5_Code_Select\").hide();\n      \n      $(\"#Ext_Org_Type_Show\").show();\n      $(\"#ORG_Code\").show();\n      $(\"#ORG_Code\").setValue(\"\");\n      $(\"#ORG_Name\").show();\n      $(\"#ORG_Name\").setValue(\"\");\n      $(\"#button_code\").show();\n      $(\"#submit0000000001\").hide();\n      $(\"#Org_Level\").hide();\n      \n      break;      \n  }\n   \n});\n\n\n//  كنترل وضعيت انتخاب نوع سازمان داخلي\n\n$(\"#ORG_Type\").setOnchange(function(OT) {\n  \n  OT = $(\"#ORG_Type\").getValue();\n  $(\"#submit0000000001\").hide();\n  $(\"#Description\").setValue(\"\",$(\"#Description\").getNumberRows(),'2');\n  $(\"#ORG_Code\").setValue(\"\");\n  \n  switch(true){\n      \n    case(OT != \"\"):\n      $('#ORG_Type').css(\"background-color\", \"\");\n      break;\n      \n    case (OT == \"\"): \n      alert (\"بايستي نوع سازمان انتخاب شود !\");\n      $('#ORG_Type').css(\"background-color\", \"orange\");\n      break;\n      \n  }\n});\n\n\n// كنترل نمايش سازمان، واحد و زير واحد\n\n$(\"#Org_Level\").setOnchange(function(OL) {\n  \n  OL = $(\"#Org_Level\").getValue();\n  $(\"#ORG_Code\").setValue(\"\");\n  $(\"#submit0000000001\").hide();\n  $(\"#Description\").setValue(\"\",$(\"#Description\").getNumberRows(),'2');\n  \n  switch (true) {\n      \n    case ( OL == \"\" ):\n      \n      $(\"#ORG_Type\").hide();\n\t  $(\"#ORG_Type_Select\").hide();\n      $(\"#ORG_L1_Code_Select\").hide();\n      $(\"#ORG_L2_Code_Select\").hide();\n      $(\"#ORG_L3_Code_Select\").hide();\n      $(\"#ORG_L4_Code_Select\").hide();\n      $(\"#ORG_L5_Code_Select\").hide();\n      \n      $(\"#Org_Type_Show\").hide();\n\t  $(\"#ORG_L1_Code_Show\").hide();\n\t  $(\"#ORG_L2_Code_Show\").hide();\n\t  $(\"#ORG_L3_Code_Show\").hide();\n\t  $(\"#ORG_L4_Code_Show\").hide();\n\t  $(\"#ORG_L5_Code_Show\").hide();\n      \n      $(\"#ORG_Code\").hide();\n\t  $(\"#ORG_Name\").hide();\n      $(\"#button_code\").hide();\n      $(\"#ORG_Code\").setValue(\"\");\n      $(\"#submit0000000001\").hide();\n      break;\n      \n    case ( OL == 1 ):\n      \n      $(\"#ORG_Type\").show();\n\t  $(\"#ORG_Type_Select\").hide();\n      $(\"#ORG_L1_Code_Select\").hide();\n      $(\"#ORG_L2_Code_Select\").hide();\n      $(\"#ORG_L3_Code_Select\").hide();\n      $(\"#ORG_L4_Code_Select\").hide();\n      $(\"#ORG_L5_Code_Select\").hide();\n      \n      $(\"#Org_Type_Show\").hide();\n\t  $(\"#ORG_L1_